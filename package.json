{
  "name": "teen_process",
  "version": "2.0.2",
  "description": "A grown up version of Node's spawn/exec",
  "keywords": [
    "child_process",
    "process management",
    "subprocess",
    "spawn",
    "exec",
    "process",
    "child"
  ],
  "bugs": {
    "url": "https://github.com/appium/node-teen_process/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/node-teen_process.git"
  },
  "license": "Apache-2.0",
  "author": "Appium Contributors",
  "main": "./index.js",
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "lib",
    "build/lib"
  ],
  "scripts": {
    "build": "babel --out-dir=build/lib lib",
    "dev": "npm run build -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:types": "tsc",
    "precommit-lint": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "prepare": "npm run build",
    "test": "mocha"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-lint"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true
  },
  "dependencies": {
    "@babel/runtime": "7.22.3",
    "bluebird": "3.7.2",
    "lodash": "4.17.21",
    "shell-quote": "1.8.1",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@appium/eslint-config-appium": "8.0.3",
    "@babel/cli": "7.21.5",
    "@babel/core": "7.22.1",
    "@babel/eslint-parser": "7.21.8",
    "@babel/plugin-transform-runtime": "7.22.4",
    "@babel/preset-env": "7.22.4",
    "@babel/register": "7.21.0",
    "@types/bluebird": "3.5.38",
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "7.1.5",
    "@types/lodash": "4.14.195",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.16",
    "@types/shell-quote": "1.7.1",
    "babel-plugin-source-map-support": "2.2.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-promise": "6.1.1",
    "lint-staged": "13.2.2",
    "mocha": "10.2.0",
    "pre-commit": "1.2.2",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "^14.17.0 || ^16.13.0 || >=18.0.0",
    "npm": ">=8"
  }
}
